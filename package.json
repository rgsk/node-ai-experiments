{
  "name": "node-ai-experiments",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "sh ./bin.dev/start.sh",
    "kill:port": "kill -9 $(lsof -t -i:4001)",
    "serve": "NODE_PATH=dist nodemon dist/app.js",
    "start": "NODE_PATH=dist node dist/app.js",
    "init:prisma": "prisma generate",
    "studio": "BROWSER=none prisma studio",
    "test": "jest",
    "build-code-runners": "sh code-runners/bin/build.sh",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "migrate:force-reset": "prisma migrate reset --force",
    "run-file": "NODE_PATH=src ts-node",
    "run-js-file": " NODE_PATH=dist node",
    "watch:file": "NODE_PATH=src ts-node-dev --respawn --transpile-only",
    "refresh:chrome": "osascript -e 'tell application \"Google Chrome\" to reload active tab of front window'",
    "watch:file:refresh:chrome": "nodemon -w views/test.ejs --exec \"yarn run:file src/examples/modifyPdf.ts && yarn refresh:chrome\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.735.0",
    "@aws-sdk/s3-request-presigner": "^3.735.0",
    "@langchain/core": "^0.3.42",
    "@modelcontextprotocol/sdk": "^1.6.1",
    "@pdf-lib/fontkit": "^1.1.1",
    "@prisma/client": "^5.16.1",
    "@types/cors": "^2.8.17",
    "ai": "^4.1.51",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "composio-core": "^0.5.18",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "firebase-admin": "^13.0.2",
    "googleapis": "^144.0.0",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "langchain": "^0.3.19",
    "multer": "^1.4.5-lts.1",
    "node-tesseract-ocr": "^2.2.1",
    "open-graph-scraper": "^6.9.0",
    "openai": "^4.52.3",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "prisma": "^5.16.1",
    "puppeteer": "^24.1.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "youtube-transcript": "^1.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/multer": "^1.4.12",
    "@types/pdf-parse": "^1.1.4",
    "nodemon": "^3.1.4",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
